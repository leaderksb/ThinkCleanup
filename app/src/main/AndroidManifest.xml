<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.co.kimsubin.ThinkCleanup">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_think"
        android:label="생각정리"
        android:roundIcon="@mipmap/ic_launcher_think_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="kr.co.kimsubin.ThinkCleanup.IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="kr.co.kimsubin.ThinkCleanup.DiaryActivity" android:label="일기" />
        <activity android:name="kr.co.kimsubin.ThinkCleanup.TodoActivity" android:label="할일" />
    </application>

</manifest>